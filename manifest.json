{
	"name": "snoozify",
	"description": "simple snooze tab extension",
	"version": "0.1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "/pages/popup.html"
	},
	"background": {
		"service_worker": "/scripts/worker.js",
		"type": "module"
	},
	"permissions": [
		"storage",
		"tabs",
		"alarms"
	]
}