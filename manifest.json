{
	"name": "snoozify",
	"description": "simple snooze tab extension",
	"version": "0.1.0",
	"icons": {
		"16": "/assets/icon.png",
		"32": "/assets/icon.png",
		"48": "/assets/icon.png",
		"128": "/assets/icon.png"
	},
	"manifest_version": 3,
	"action": {
		"default_popup": "/pages/popup.html"
	},
	"background": {
		"service_worker": "/scripts/worker.js",
		"type": "module"
	},
	"permissions": [
		"storage",
		"tabs",
		"alarms",
		"notifications"
	]
}